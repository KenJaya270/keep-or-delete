{
  "expo": {
    "name": "KeepOrDelete",
    "slug": "keepordelete",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/keepordelete.png",
    "scheme": "keepordelete",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/keepordelete.png",
        "backgroundImage": "./assets/images/keepordelete.png",
        "monochromeImage": "./assets/images/keepordelete.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ],
      "package": "com.kenjaya124.keepordelete"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/keepordelete.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/keepordelete.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow this app to access your photos to manage and delete images."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "34c97d84-d5d6-4f99-9ade-2c93a999a921"
      }
    }
  }
}
